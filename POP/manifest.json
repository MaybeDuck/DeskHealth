{
    "name": "DeskHealth",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
  
      "default_popup": "HTML/popup.html",
      "default_title": "DeakHealth Settings"
    }, 
    "commands":{
      "_execute_action":{
        "suggested_key":{
          "default": "Ctrl+Shift+Y"
        }
      }
    },
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["tabs","alarms","storage"],
    
    "icons": { "16": "ICONS/mainIcon.png"}
   
    
  }